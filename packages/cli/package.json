{
  "name": "@magidoc/cli",
  "description": "Magidoc CLI application responsible for generating GraphQL documentation websites.",
  "private": false,
  "version": "2.5.1",
  "type": "module",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "cli",
    "node",
    "magidoc"
  ],
  "homepage": "https://magidoc-org.github.io/magidoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/cli"
  },
  "bin": {
    "magidoc": "./build/index.js"
  },
  "scripts": {
    "start": "pnpm build:rollup && node build/index.js",
    "build": "run-p build:*",
    "build:rollup": "rollup -c",
    "build:tsc": "tsc --build tsconfig.build.json",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@magidoc/plugin-starter-variables": "workspace:^2.5.1",
    "@magidoc/rollup-plugin-fetch-gql-schema": "workspace:^2.5.1",
    "@magidoc/rollup-plugin-parse-gql-schema": "workspace:^2.5.1",
    "axios": "^0.27.2",
    "chokidar": "^3.5.3",
    "commander": "^9.3.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.1.0",
    "listr2": "^4.0.5",
    "lodash": "^4.17.21",
    "sirv": "^2.0.2",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.5",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.3",
    "jest": "^28.1.3",
    "jest-extended": "^3.0.1",
    "rollup": "^2.76.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.7"
  }
}
