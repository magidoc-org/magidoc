{
  "name": "@magidoc/cli",
  "description": "Magidoc CLI application responsible for generating GraphQL documentation websites.",
  "private": false,
  "version": "2.6.0",
  "type": "module",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "cli",
    "node",
    "magidoc"
  ],
  "homepage": "https://magidoc-org.github.io/magidoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/cli"
  },
  "bin": {
    "magidoc": "./build/index.js"
  },
  "scripts": {
    "start": "pnpm build:rollup && node build/index.js",
    "build": "run-p build:*",
    "build:rollup": "rollup -c",
    "build:tsc": "tsc --build tsconfig.build.json",
    "test": "vitest run --coverage",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@magidoc/plugin-starter-variables": "workspace:^2.6.0",
    "@magidoc/rollup-plugin-fetch-gql-schema": "workspace:^2.6.0",
    "@magidoc/rollup-plugin-parse-gql-schema": "workspace:^2.6.0",
    "axios": "^0.27.2",
    "chokidar": "^3.5.3",
    "commander": "^9.4.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.1.0",
    "listr2": "^4.0.5",
    "lodash": "^4.17.21",
    "sirv": "^2.0.2",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.6",
    "c8": "^7.12.0",
    "rollup": "^2.77.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vitest": "^0.18.1"
  },
  "engines": {
    "node": ">=16.7"
  }
}
