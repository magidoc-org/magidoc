{
  "name": "@magidoc/cli",
  "description": "Magidoc CLI application responsible for generating GraphQL documentation websites.",
  "private": false,
  "version": "4.1.3",
  "type": "module",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "cli",
    "node",
    "magidoc"
  ],
  "homepage": "https://magidoc.js.org/introduction/welcome",
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/cli"
  },
  "bin": {
    "magidoc": "./build/index.js"
  },
  "scripts": {
    "start": "pnpm build:rollup && pnpm execute",
    "execute": "node build/index.js",
    "build": "run-p build:*",
    "build:rollup": "rollup -c",
    "build:tsc": "tsc --build tsconfig.build.json",
    "test": "vitest run --coverage",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@magidoc/plugin-starter-variables": "workspace:^",
    "@magidoc/rollup-plugin-gql-schema": "workspace:^",
    "axios": "^1.6.7",
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.2.0",
    "listr2": "^7.0.2",
    "lodash": "^4.17.21",
    "sirv": "^2.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "@vitest/coverage-v8": "^0.34.6",
    "rollup": "^4.10.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "build"
  ]
}
