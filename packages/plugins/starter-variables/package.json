    "test": "jest",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.42",
    "jest": "^28.1.1",
    "lodash": "^4.17.21",
    "rollup": "^2.75.6",
    "ts-jest": "^28.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "optionalDependencies": {
    "zod": "^3.17.3"
  }
}
