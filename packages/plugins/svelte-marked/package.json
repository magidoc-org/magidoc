{
  "name": "@magidoc/plugin-svelte-marked",
  "description": "A markdown parser library that renders to svelte components.",
  "type": "module",
  "license": "MIT",
  "version": "2.14.4",
  "svelte": "index.js",
  "private": false,
  "keywords": [
    "svelte",
    "magidoc",
    "browser",
    "marked",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/plugins/svelte-marked"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm package",
    "test": "pnpm check",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "release": "cd package && pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "marked": "^4.2.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0-next.89",
    "@sveltejs/kit": "1.0.0-next.563",
    "@sveltejs/package": "1.0.0-next.6",
    "@types/marked": "^4.0.7",
    "@types/prismjs": "^1.26.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.20",
    "tslib": "^2.4.1",
    "typescript": "~4.9.3",
    "vite": "^3.2.4"
  }
}
