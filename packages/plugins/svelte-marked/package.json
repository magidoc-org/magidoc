{
  "name": "@magidoc/plugin-svelte-marked",
  "description": "A markdown parser library that renders to svelte components.",
  "type": "module",
  "license": "MIT",
  "version": "2.5.2",
  "svelte": "index.js",
  "private": false,
  "keywords": [
    "svelte",
    "magidoc",
    "browser",
    "marked",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/plugins/svelte-marked"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm package",
    "test": "pnpm check",
    "package": "svelte-kit package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "release": "cd package && pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "marked": "^4.0.18"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0-next.63",
    "@sveltejs/kit": "1.0.0-next.391",
    "@types/marked": "^4.0.3",
    "@types/prismjs": "^1.26.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.12",
    "tslib": "^2.4.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.2"
  }
}
