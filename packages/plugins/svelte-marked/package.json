{
  "name": "@magidoc/plugin-svelte-marked",
  "description": "A markdown parser library that renders to svelte components.",
  "type": "module",
  "license": "MIT",
  "version": "3.1.0",
  "svelte": "index.js",
  "private": false,
  "keywords": [
    "svelte",
    "magidoc",
    "browser",
    "marked",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/plugins/svelte-marked"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm package",
    "test": "pnpm check",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "release": "cd package && pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "marked": "^4.2.12"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.2",
    "@sveltejs/kit": "1.2.2",
    "@sveltejs/package": "1.0.2",
    "@types/marked": "^4.0.8",
    "@types/prismjs": "^1.26.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.2",
    "svelte2tsx": "^0.6.0",
    "tslib": "^2.4.1",
    "typescript": "~4.9.4",
    "vite": "^4.0.4"
  }
}
