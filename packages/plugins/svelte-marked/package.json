{
  "name": "@magidoc/plugin-svelte-marked",
  "description": "A markdown parser library that renders to svelte components.",
  "type": "module",
  "license": "MIT",
  "version": "4.1.2",
  "svelte": "./build/index.js",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "svelte": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "private": false,
  "keywords": [
    "svelte",
    "magidoc",
    "browser",
    "marked",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/plugins/svelte-marked"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package --output build",
    "test": "pnpm check",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "github-slugger": "^2.0.0",
    "marked": "^11.1.0",
    "svelte": "^4.2.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.25.2",
    "@sveltejs/package": "2.2.2",
    "@types/marked": "^6.0.0",
    "@types/prismjs": "^1.26.1",
    "svelte-check": "^3.6.2",
    "svelte2tsx": "^0.6.23",
    "tslib": "^2.6.2",
    "typescript": "~5.3.3",
    "vite": "^5.0.10"
  }
}
