  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/prettier": "^2.6.0",
    "graphql-query-compress": "^1.2.4",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "rollup": "^2.70.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "graphql": "^16.3.0"
  }
