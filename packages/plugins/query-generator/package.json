{
  "name": "@magidoc/plugin-query-generator",
  "description": "A browser and nodeJS compatible plugin that allows to generate GraphQL Queries and Variables",
  "private": false,
  "version": "3.0.0",
  "type": "module",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "graphql",
    "query",
    "generator",
    "node",
    "browser"
  ],
  "homepage": "https://magidoc.js.org/plugins/graphql-query-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/magidoc-org/magidoc.git",
    "directory": "packages/plugins/query-generator"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:rollup": "rollup -c",
    "build:tsc": "tsc --build tsconfig.build.json",
    "test": "vitest run --coverage",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.190",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@vitest/coverage-c8": "^0.25.3",
    "graphql-query-compress": "^1.2.4",
    "rollup": "^3.5.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  }
