    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.3.1",
    "typescript": "~4.6.3"
  },
  "peerDependencies": {
    "prismjs": "^1.27.0"
  }
}
