{
  "name": "@magidoc/plugin-svelte-prismjs",
  "description": "A Svelte component library providing an easy to use component to display PrismJS code blocks",
  "type": "module",
  "license": "MIT",
  "version": "0.6.1",
  "svelte": "index.js",
  "private": false,
  "keywords": [
    "svelte",
    "svelte-kit",
    "prism",
    "prismjs",
    "magidoc",
    "browser"
