{
  "name": "@magidoc/docs",
  "version": "1.1.3",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "docs",
    "magidoc"
  ],
  "scripts": {
    "build": "pnpm --filter @magidoc/cli start generate --file $(pwd)/magidoc.mjs --stacktrace",
    "preview": "pnpm --filter @magidoc/cli start preview --file $(pwd)/magidoc.mjs --stacktrace",
    "release": "gh-pages --dist build --dotfiles true"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0"
  }
}
